<p id="notice"><%= notice %></p>

<h1>Capacities</h1>

<table>
  <thead>
    <tr>
      <th>Sector</th>
      <th>Year</th>
      <th>Month</th>
      <th>Percentage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @capacities.each do |capacity| %>
      <tr>
        <td><%= capacity.sector.name %></td>
        <td><%= capacity.year %></td>
        <td><%= capacity.month %></td>
        <td><%= capacity.percentage %></td>
        <td><%= link_to 'Show', capacity %></td>
        <td><%= link_to 'Edit', edit_capacity_path(capacity) %></td>
        <td><%= link_to 'Destroy', capacity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Capacity', new_capacity_path %>
