<p id="notice"><%= notice %></p>

<h1><%= t('.title') %></h1>

<table>
  <thead>
    <tr>
      <th><%= t('activerecord.attributes.sector.name') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sectors.each do |sector| %>
      <tr>
        <td><%= sector.name %></td>
        <td><%= link_to t(:ver), sector %></td>
        <td><%= link_to t(:editar), edit_sector_path(sector) %></td>
        <td><%= link_to t(:eliminar), sector, method: :delete, data: { confirm: t(:esta_seguro) } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('sectors.new.button'), new_sector_path %>
